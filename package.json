{
  "name": "x-is-equal",
  "version": "1.2.0",
  "description": "Check equality of multiple values/objects/arrays",
  "author": "Vincent Sj√∂gren",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "umd:main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "default": "./dist/index.modern.js"
  },
  "scripts": {
    "build": "del-cli dist && microbundle --compress --no-sourcemap --strict && npm run test:build",
    "dev": "microbundle watch",
    "lint": "eslint src/** __tests__/** --ext .ts",
    "format": "prettier --write .",
    "test": "jest index",
    "test:build": "jest build",
    "test:coverage": "jest --coverage",
    "pre-commit": "npm test && pretty-quick --staged",
    "pre-push": "npm test && npm run lint",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsjo/x-is-equal.git"
  },
  "keywords": [
    "equality",
    "cjs",
    "esm"
  ],
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.1",
    "husky": "^8.0.0",
    "microbundle": "latest",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3"
  },
  "dependencies": {
    "x-is-type": "^1.2.1"
  }
}
